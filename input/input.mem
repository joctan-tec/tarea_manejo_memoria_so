# Asignación de memoria para diferentes variables
ALLOC variable1 35
ALLOC variable2 128

# Reasignación de variable1 a un nuevo tamaño
REALLOC variable1 128

ALLOC variable3 64

# Liberación de variable2
FREE variable2

# Estado actual de las asignaciones
PRINT

# Reasignación de variable1 a un tamaño mayor
REALLOC variable1 256

# Liberación de variable1
FREE variable1

# Estado final de las asignaciones
PRINT

